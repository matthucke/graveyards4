= form_for @county do |f|
  - if @county.errors.any?
    #error_explanation
      %h2= "#{pluralize(@county.errors.count, "error")} prohibited this county from being saved:"
      %ul
        - @county.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :state
    = f.text_field :state
  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :type_name
    = f.text_field :type_name
  .field
    = f.label :path
    = f.text_field :path
  .field
    = f.label :full_path
    = f.text_field :full_path
  .actions
    = f.submit 'Save'
